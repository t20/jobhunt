.hero-unit
  h2.name= company.name
  != linkTo('Edit this company', editCompanyPath(params.id), {class: 'btn pull-right'})

h2 Company Properties
each property, key in company
  if key !== 'saved'
    .row.list-item
      .span4
        h3= key
      .span8
        h3
          i.icon-list-alt
          span(class=key)= property
